configuration {
    modi: "drun";
    font: "DaddyTimeMono Nerd Font 12";
    show-icons: true;
    sidebar-mode: false;
    terminal: "kitty";
    sort: false;
    lines: 2;
    scroll-method: 1;
    window-format: "{c}     {w}";
    kb-row-tab: ""; 
    kb-mode-previous: "Shift+Tab";
    kb-row-up: "Up";        
    timeout {
        action: "kb-cancel";
        delay:  0;
    }
}
@theme "nyarch"
